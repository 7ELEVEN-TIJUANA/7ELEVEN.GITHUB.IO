body{
        background:white;
}
h1{
        color: #DF013A;
        font-family: Monaco;
        font-size: 40px;
        text-align: center;
}
p{
        color: #FFFFFF;
        font-family: Arial;
        font-size: 10px;
        text-align: center;
        font-weight: bold;
        font-style: italic;
}
table{
        color: #FFFFFF;
        text-align: center;
        font-family: Arial;
        font-size: 20px;
        font-weight: bold;
        font-style: italic;
        width: 90%;
        height: 60%;
        border: 2;
        border-color: white;
        border-style: solid;
        margin-left: auto;
        margin-right: auto;
        border-collapse: collapse;
}
tr{
        color: #FFFFFF;
        font-family: Arial;
        font-size: 50px;
        text-align: center;
        background-color: #FFFFFF;
        border-color: white;
        border-style: solid;
        
}
th{
        color: #FFFFFF;
        font-family: Arial;
        font-size: 50px;
        text-align: center;
        background-color: #FFFFFF;
        border-color: white;
        border-style: solid;
}        `
td{
        color: #FFFFFF;
        font-family: Arial;
        font-size: 50px;
        text-align: center;
        background-color: #FFFFFF;
        border-color: white;
        border-style: solid;
}

/*Esta clase es para quitar el borde y fondo a las tablas*/
.sin{
        border: 0;
        border-style: transparent;
        background-color: transparent;
}
/*Esta clase es para modificar la imagen de logo*/
.logo{
        height: 200px;
        width: 300px; 
}
/*Esta clase es para modificar las imagenes de la pagina principal*/
.pri{
        height: 300px;
        width: 300px; 
}
/*Esta clase es para modificar las imagenes de los iconos*/
.ico{
        height: 140px;
        width: 140px; 
}
/*Esta clase es para modificar texto de la pagina Historia*/
.titu{
        color: #DC0000 ;
        font-family: Arial;
        font-size: 25px;
        text-align: center;
        font-weight: bold;
}
.font{
        color: #FFFFFF;
        font-family: Arial;
        font-size: 15px;
        text-align: center;
        font-weight: bold;
        font-style: italic;
}
/*Esta clase es para modificar pagina producto*/
.pro{
        height: 250px;
        width: 300px; 
}
.tab{
        color: #FFFFFF;
        text-align: center;
        font-family: Arial;
        font-size: 20px;
        font-weight: bold;
        border: 2;
        border-color: white;
        border-style: solid;
        margin-left: auto;
        margin-right: auto;
        border-collapse: collapse;
        background: #FF3F3F ;
}
.div1{
	background:linear-gradient(to bottom, #239B56, #ABEBC6);
	border: 4px solid #145A32;
}
.div2{
	background:linear-gradient(to bottom, #FFE400, #FAD7A0);
	border: 4px solid #FFC300;
}
.div3{
	background:linear-gradient(to bottom, #873600, #AF601A);
	border: 4px solid #6E2C00;
}
.div4{
	background:linear-gradient(to bottom, #515A5A, #707B7C);
	border: 4px solid #424949;
}